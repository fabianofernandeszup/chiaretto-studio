      "type": "string",
      "enum": [
        "v3"
      ]
    },
    "kind": {
      "type": "string",
      "enum": [
        "action"
      ]
    },
    "metadata": {
      "$ref": "#/$defs/commons_metadata"
    },
    "spec": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "shell"
          ]
        },
        "justification": {
          "type": "string"
        },
        "docs": {
          "$ref": "#/$defs/commons_docs"
