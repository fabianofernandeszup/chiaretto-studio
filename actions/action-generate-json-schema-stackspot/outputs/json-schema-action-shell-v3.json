        },
        "computed-inputs": {
          "type": "object"
        },
        "requires": {
          "$ref": "#/$defs/action_requires_v3"
        },
        "outputs": {
          "$ref": "#/$defs/commons_outputs"
        }
      },
      "required": [
        "type",
        "shell",
        "docs"
      ]
    }
  },
  "required": [
    "schema-version",
    "kind",
    "metadata",
    "spec"
  ],
  "$defs": {
    "commons_metadata": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "pattern": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
        },
        "display-name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "version": {
          "type": "string",
          "pattern": "^\\d+\\.\\d+\\.\\d+$"
        },
        "picture": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "display-name",
        "description",
        "version"
      ],
      "additionalProperties": false
    },
    "commons_docs": {
      "type": "object",
      "minProperties": 1,
      "additionalProperties": false,
      "properties": {
        "en-us": {
          "type": "string",
          "pattern": "^([a-zA-Z0-9-_.]+[\\\\\\/])*([a-zA-Z0-9-_.]+\\.md)$"
        },
        "pt-br": {
          "type": "string",
          "pattern": "^([a-zA-Z0-9-_.]+[\\\\\\/])*([a-zA-Z0-9-_.]+\\.md)$"
        }
      }
    },
    "commons_inputs_v3": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "bool",
              "int",
              "list",
              "multiselect",
              "object",
              "password",
              "required-connection",
              "generated-connection",
              "select",
              "text",
              "textarea"
            ]
          }
        },
        "required": [
          "type"
        ],
        "if": {
          "properties": {
            "type": {
              "const": "bool"
            }
          }
        },
        "then": {
          "$ref": "#/$defs/commons_inputs_v3_input_bool"
        },
        "else": {
          "if": {
            "properties": {
              "type": {
                "const": "int"
              }
            }
          },
          "then": {
            "$ref": "#/$defs/commons_inputs_v3_input_int"
          },
          "else": {
            "if": {
              "properties": {
                "type": {
                  "const": "list"
                }
              }
            },
            "then": {
              "$ref": "#/$defs/commons_inputs_v3_input_list"
            },
            "else": {
              "if": {
                "properties": {
                  "type": {
                    "const": "multiselect"
                  }
                }
              },
              "then": {
                "$ref": "#/$defs/commons_inputs_v3_input_multiselect"
              },
              "else": {
                "if": {
                  "properties": {
                    "type": {
                      "const": "object"
                    }
                  }
                },
                "then": {
                  "$ref": "#/$defs/commons_inputs_v3_input_object"
                },
                "else": {
                  "if": {
                    "properties": {
                      "type": {
                        "const": "password"
                      }
                    }
                  },
                  "then": {
                    "$ref": "#/$defs/commons_inputs_v3_input_password"
                  },
                  "else": {
                    "if": {
                      "properties": {
                        "type": {
                          "const": "required-connection"
                        }
                      }
                    },
                    "then": {
                      "$ref": "#/$defs/commons_inputs_v3_input_required_connection"
                    },
                    "else": {
                      "if": {
                        "properties": {
                          "type": {
                            "const": "select"
                          }
                        }
                      },
                      "then": {
                        "$ref": "#/$defs/commons_inputs_v3_input_select"
                      },
                      "else": {
                        "if": {
                          "properties": {
                            "type": {
                              "const": "text"
                            }
                          }
                        },
                        "then": {
                          "$ref": "#/$defs/commons_inputs_v3_input_text"
                        },
                        "else": {
                          "if": {
                            "properties": {
                              "type": {
                                "const": "generated-connection"
                              }
                            }
                          },
                          "then": {
                            "$ref": "#/$defs/commons_inputs_v3_input_generated_connection"
                          },
                          "else": {
                            "if": {
                              "properties": {
                                "type": {
                                  "const": "textarea"
                                }
                              }
                            },
                            "then": {
                              "$ref": "#/$defs/commons_inputs_v3_input_textarea"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
